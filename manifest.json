{
  "manifest_version": 3,
  "name": "Bug Report Generator",
  "version": "2.5",
  "description": "Generate bug reports with screenshots matching the Zoonou template",
 "permissions": [
    "activeTab",
    "downloads",
    "scripting",
    "clipboardWrite",
	"storage"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
